{
  "name": "ts-react-redux-router-saga",
  "version": "1.0.0",
  "description": "This simple and clear boilerplate for start your project with react-redux ecosystem.",
  "keywords": [
    "ts",
    "typescript",
    "react",
    "redux",
    "router",
    "saga"
  ],
  "main": "index.js",
  "author": "snatvb",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve --config webpack.config.js",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "format:stage": "npx pretty-quick --staged",
    "postinstall": "husky install",
    "release": "standard-version",
    "prepare": "husky install",
    "commit": "cz"
  },
  "lint-staged": {
    "*": [
      "yarn format:stage"
    ]
  },
  "dependencies": {
    "normalize.css": "8.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "7.2.4",
    "react-router-dom": "5.2.0",
    "redux-saga": "1.1.3",
    "typesafe-actions": "5.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.14.8",
    "@babel/preset-env": "7.14.8",
    "@babel/preset-react": "7.14.5",
    "@commitlint/cli": "16.1.0",
    "@commitlint/config-conventional": "16.0.0",
    "@types/react": "17.0.14",
    "@types/react-dom": "17.0.9",
    "@types/react-redux": "7.1.18",
    "@types/react-router-dom": "5.1.8",
    "@types/redux-saga": "0.10.5",
    "babel-loader": "8.2.2",
    "commitizen": "4.2.4",
    "cross-env": "7.0.3",
    "css-loader": "6.2.0",
    "cz-conventional-changelog": "3.3.0",
    "html-loader": "2.1.2",
    "html-webpack-plugin": "5.3.2",
    "husky": "^7.0.0",
    "lint-staged": "12.2.2",
    "mini-css-extract-plugin": "2.1.0",
    "node-sass": "6.0.1",
    "prettier": "2.3.2",
    "pretty-quick": "^3.1.1",
    "sass": "1.35.2",
    "sass-loader": "12.1.0",
    "standard-version": "9.3.2",
    "style-loader": "3.2.1",
    "ts-loader": "9.2.3",
    "typescript": "4.3.5",
    "webpack": "5.46.0",
    "webpack-cli": "4.7.2",
    "webpack-dev-server": "3.11.2"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
